<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!--    Inherited Product Category form-->
    <record id="product_category_form_view_inherit_deardigital_webship_connector" model="ir.ui.view">
        <field name="name">product.category.form.view.inherit.deardigital.webship.connector</field>
        <field name="model">product.category</field>
        <field name="inherit_id" ref="product.product_category_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='parent_id']" position="after">
                <field name="ignored_in_webship"/>
            </xpath>
        </field>
    </record>
</odoo>